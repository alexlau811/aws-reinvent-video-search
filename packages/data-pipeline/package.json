{
  "name": "@aws-reinvent-search/data-pipeline",
  "version": "1.0.0",
  "description": "Data processing pipeline for AWS re:Invent videos",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "create-sample-db": "tsx src/create-sample-db.ts",
    "create-production-db": "tsx src/create-production-db.ts",
    "create-production-db:simple": "tsx src/create-production-simple.ts",
    "build-real-db": "tsx src/build-real-db-simple.ts",
    "create-production-db:full": "tsx src/create-production-db.ts --backup --compression",
    "deploy-production-db": "tsx src/deploy-production-db.ts",
    "verify-production-db": "tsx -e \"import { ProductionDatabaseService } from './src/database/ProductionDatabaseService.js'; const db = new ProductionDatabaseService(process.argv[2] || '../client-app/public/database/reinvent-videos-prod.db'); db.verifyDatabase().then(console.log).finally(() => db.close())\""
  },
  "dependencies": {
    "@aws-reinvent-search/shared": "file:../shared",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-cloudfront": "^3.0.0",
    "better-sqlite3": "^9.0.0",
    "fast-check": "^3.0.0",
    "yt-dlp-wrap": "^2.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.0.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "typescript": "^5.0.0"
  }
}